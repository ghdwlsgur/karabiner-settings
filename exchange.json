{
	"title": "tab 대소문자 변환",
	"rules": [
		{	
			"description": "exchange left/right_command into left/right_option",
			"manipulators": [
			{ 
				"type": "basic",    
				"from": { 
					"key_code": "left_command",
          "modifiers": {
						"optional": [ "any" ] }
          
				},
        "to": [ 
					{ 
						"key_code": "left_option"
					}
        ]
      },	
			{ 
				"type": "basic",    
				"from": { 
					"key_code": "left_option",
          "modifiers": {
						"optional": [ "any" ] }
          
				},
        "to": [ 
					{ 
						"key_code": "left_command"
					}
        ]
      },
			{ 
				"type": "basic",    
				"from": { 
					"key_code": "right_command",
          "modifiers": {
						"optional": [ "any" ] }
          
				},
        "to": [ 
					{ 
						"key_code": "right_option"
					}
        ]
      },	
			{ 
				"type": "basic",    
				"from": { 
					"key_code": "right_option",
          "modifiers": {
						"optional": [ "any" ] }
          
				},
        "to": [ 
					{ 
						"key_code": "right_command"
					}
        ]
      }
		]
	}]
}
